<%= render( :partial => "search_result",
            :locals => {
              :objects => @users,
              :model => 'user',
              :path => lambda { |school_id, id| user_path(school_id, id) },
              :attributes => ["displayName", "uid"] } ) %>

<%= render( :partial => "search_result",
            :locals => {
              :objects => @roles,
              :model => 'role',
              :path => lambda { |school_id, id| role_path(school_id, id) },
              :attributes => ["displayName"] } ) %>

<%= render( :partial => "search_result",
            :locals => {
              :objects => @groups,
              :model => 'group',
              :path => lambda { |school_id, id| group_path(school_id, id) },
              :attributes => ["displayName", "cn"] } ) %>

<%= render( :partial => "search_result",
            :locals => {
              :objects => @devices,
              :model => 'device',
              :path => lambda { |school_id, id| "/devices/#{school_id}/devices/#{id}" },
              :attributes => ["puavoHostname"] } ) %>
