<% page_title t('titles.organisation'), t('titles.wlan') %>

<h1><%= t('shared.form_wlan.title') %></h1>

<%= form_for( @organisation,
             :html => { :multipart => true },
             :url  => wlan_update_organisation_path ) do |f| %>

<%= render :partial => 'shared/form_wlan', :locals => { :f => f } %>

<% end %>

<% content_for :javascript do %>
<%= javascript_include_tag "wlan" %>
<% end %>
